<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tour"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tour"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RomanTherat"
            android:label="@string/ThDisl"/>
        <activity android:name=".Jacob"
            android:label="@string/jacob"/>
        <activity android:name=".Sabastia"
            android:label="Sabastia Ruins"/>
        <activity android:name=".OldCityKhan"
            android:label="Al- Khan"/>
    </application>

</manifest>